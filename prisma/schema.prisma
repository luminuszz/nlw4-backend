datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

generator client {
    provider = "prisma-client-js"
}

model User {
    id        String   @id @default(uuid())
    name      String
    email     String   @unique
    password  String
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt

}

model Survey {
    id          String   @id @default(uuid())
    title        String
    description String
    createdAt   DateTime @default(now())
    updatedAt   DateTime @updatedAt
}
