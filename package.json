{
	"name": "nlw4-backend",
	"version": "0.0.1",
	"description": "",
	"author": "",
	"private": true,
	"license": "UNLICENSED",
	"scripts": {
		"prebuild": "rimraf dist",
		"build": "nest build",
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"start": "nest start",
		"start:dev": "nest start --watch",
		"start:debug": "nest start --debug --watch",
		"start:prod": "node dist/main",
		"lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
		"test": "export NODE_ENV=testing && jest --expand  --verbose  ",
		"test:watch": "jest --watch",
		"test:cov": "jest --coverage",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"test:e2e": "yarn open_api_container && cp ./.env.test2e .env && jest --detectOpenHandles --config ./test/jest-e2e.js",
		"prisma": "docker exec app_api npx prisma",
		"prisma-studio": "export DB_HOST=localhost &&  npx prisma studio --port=8000 --hostname=localhost",
		"start:docker:dev": "export NODE_ENV=development && nest start --watch",
		"open_api_container": "docker exec  app_api bash",
		"dev": "docker-compose --env-file=./.env.development up"
	},
	"dependencies": {
		"@nestjs/common": "^7.6.15",
		"@nestjs/core": "^7.6.15",
		"@nestjs/mapped-types": "^0.4.1",
		"@nestjs/platform-express": "^7.6.15",
		"@prisma/client": "^2.25.0",
		"@types/dotenv": "^8.2.0",
		"bcrypt": "^5.0.1",
		"dotenv": "^10.0.0",
		"faker": "^5.5.3",
		"prisma": "^2.25.0",
		"reflect-metadata": "^0.1.13",
		"rimraf": "^3.0.2",
		"rxjs": "^6.6.6",
		"shelljs": "^0.8.4",
		"uuid": "^8.3.2"
	},
	"devDependencies": {
		"@nestjs/cli": "^7.6.0",
		"@nestjs/schematics": "^7.3.0",
		"@nestjs/testing": "^7.6.15",
		"@types/bcrypt": "^5.0.0",
		"@types/express": "^4.17.11",
		"@types/faker": "^5.5.6",
		"@types/jest": "^26.0.22",
		"@types/node": "^14.14.36",
		"@types/shelljs": "^0.8.8",
		"@types/supertest": "^2.0.10",
		"@types/uuid": "^8.3.0",
		"@typescript-eslint/eslint-plugin": "^4.19.0",
		"@typescript-eslint/parser": "^4.19.0",
		"add": "^2.0.6",
		"eslint": "^7.22.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-config-standard": "^16.0.3",
		"eslint-plugin-prettier": "^3.4.0",
		"jest": "^26.6.3",
		"prettier": "^2.3.1",
		"supertest": "^6.1.3",
		"ts-jest": "^26.5.4",
		"ts-loader": "^8.0.18",
		"ts-node": "^9.1.1",
		"tsconfig-paths": "^3.9.0",
		"typescript": "^4.2.3",
		"yarn": "^1.22.10"
	}
}
